{
  "name": "portalen-frontend",
  "description": "Portal frontpage for Telemark fylkeskommune",
  "version": "1.1.9",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/portalen-craco.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/portalen-craco/issues"
  },
  "dependencies": {
    "@babel/polyfill": "7.10.4",
    "@date-io/date-fns": "2.8.1",
    "@hot-loader/react-dom": "16.13.0",
    "@loadable/component": "5.13.1",
    "@material-ui/core": "4.11.0",
    "@material-ui/pickers": "3.2.10",
    "core-js": "3.6.5",
    "date-fns": "2.16.0",
    "dayjs": "1.8.34",
    "isomorphic-unfetch": "3.0.0",
    "object.omit": "3.0.0",
    "prop-types": "15.7.2",
    "querystringify": "2.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet-async": "1.0.6",
    "react-hot-loader": "4.12.21",
    "react-markdown": "4.3.1",
    "react-redux": "7.2.1",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@loadable/babel-plugin": "5.13.0",
    "customize-cra": "1.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "http-proxy-middleware": "1.0.5",
    "husky": "4.2.5",
    "prettier": "2.1.1",
    "pretty-quick": "3.0.0",
    "react-app-rewired": "2.1.6",
    "react-scripts": "3.4.3",
    "webpack-bundle-analyzer": "3.9.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "now-dev": "BROWSER=none react-scripts start",
    "dev": "now dev",
    "deploy": "now --prod",
    "build": "NODE_ENV=production react-scripts build",
    "analyze": "ANALYZE=1 NODE_ENV=production react-app-rewired build",
    "lint": "eslint --no-eslintrc -c .eslintrc src api",
    "test": "react-scripts test",
    "format": "prettier --write \"src/**/*.js\" \"api/**/*.js\" \"config-overrides.js\"",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=12.14.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
